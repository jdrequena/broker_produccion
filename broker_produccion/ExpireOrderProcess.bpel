                    <copy ignoreMissingFromData="yes">
                      <from>$Invoke_LOG_ORCHESTRATOR_PRODUCT_SELECT_Output.LOG_ORCHESTRATOR_PRODUCT_SELECTOutputCollection/ns19:LOG_ORCHESTRATOR_PRODUCT_SELECTOutput[$productCounter]/ns19:START_DATE</from>
