         xmlns:ns11="http://tigo.com/enterprise/businessEntity/product/basic/v1/schema"
         xmlns:ns12="http://tigo.com/serviceEntity/provisioning/offeringDetail/v1/schema"
         xmlns:ns9="http://tigo.com/enterprise/businessEntity/client/basic/v1/schema"
         xmlns:ns8="http://tigo.com/enterprise/taskService/provisioning/v1/schema"
         xmlns:ns10="http://tigo.com/enterprise/serviceEntity/provisioning/order/v1/schema"
         xmlns:ns15="http://tigo.com/commons/generalResponse/v1/schema"
