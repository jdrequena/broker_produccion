  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      PARTNERLINKS                                                      
      List of services participating in this BPEL process               
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <partnerLinks>
    <!-- 
      The 'client' role represents the requester of this service. It is 
      used for callback. The location and correlation information associated
      with the client role are automatically set using WS-Addressing.
    -->
    <partnerLink name="expireorderprocess_client" partnerLinkType="client:ExpireOrderProcess"
                 myRole="BasicOrderPort"/>
    <partnerLink name="LOG_ORCHESTRATOR_M_ADAPTER"
                 partnerLinkType="ns1:LOG_ORCHESTRATOR_M_ADAPTER_plt"
                 partnerRole="LOG_ORCHESTRATOR_M_ADAPTER_role"/>
    <partnerLink name="ExpireMediator.ExpireMediator"
                 partnerLinkType="ns16:ExpireMediator.ExpireMediator"
                 partnerRole="ProvisioningPort"/>
    <partnerLink name="FaultNotificationService" partnerLinkType="ns17:FaultNotificationService"
                 partnerRole="FaultNotification_Port"/>
    <partnerLink name="LOG_ORCHESTRATOR_PRODUCT_SELECT"
                 partnerLinkType="ns18:LOG_ORCHESTRATOR_PRODUCT_SELECT_plt"
                 partnerRole="LOG_ORCHESTRATOR_PRODUCT_SELECT_role"/>
  </partnerLinks>
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      VARIABLES                                                        
      List of messages and XML documents used within this BPEL process 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <variables>
    <!-- Reference to the message passed as input during initiation -->
    <variable name="inputVariable" messageType="client:BasicExpireOrderRequest_msg"/>
    <!-- Reference to the message that will be returned to the requester-->
    <variable name="outputVariable" messageType="client:BasicExpireOrderResponse_msg"/>
    <variable name="Invoke_LogMSelect_Output" messageType="ns1:LogOrchestratorMCollection_msg"/>
    <variable name="Invoke_LogMSelect_Input"
              messageType="ns1:LOG_ORCHESTRATOR_M_ADAPTERSelect_inputParameters"/>
    <variable name="Invoke_LogProductSelect_Output"
              messageType="ns5:LogOrchestratorProductCollection_msg"/>
    <variable name="Invoke_ProvideProcess_Input" messageType="ns7:BasicProvisioningRequest_msg"/>
    <variable name="Invoke_ProvideProcess_Output" messageType="ns7:BasicProvisioningResponse_msg"/>
    <variable name="generalResponse"
              messageType="client:GeneralResponse_msg"/>
    <variable name="Invoke_LogProductSelect_Input"
              messageType="ns5:LOG_ORCHESTRATOR_DETAILSelect_inputParameters"/>
    <variable name="Invoke_ProvisioningService_Input"
              messageType="ns16:BasicProvisioningRequest_msg"/>
    <variable name="Invoke_ProvisioningService_Output"
              messageType="ns16:BasicProvisioningResponse_msg"/>
    <variable name="Invoke_LOG_ORCHESTRATOR_PRODUCT_SELECT_Input"
              messageType="ns18:LOG_ORCHESTRATOR_PRODUCT_SELECTInput_msg"/>
    <variable name="Invoke_LOG_ORCHESTRATOR_PRODUCT_SELECT_Output"
              messageType="ns18:LOG_ORCHESTRATOR_PRODUCT_SELECTOutputCollection_msg"/>
    <variable name="Invoke_CreateFault_Input"
              messageType="ns17:CreateFaultRequest_msg"/>
    <variable name="Invoke_CreateFault_Output"
              messageType="ns17:CreateFaultResponse_msg"/>
    <variable name="uti" type="xsd:string"/>
    <variable name="globalFaultVar"
              element="ns15:GeneralResponse"/>
  </variables>
  <faultHandlers>
    <catch faultName="ns17:fault">
      <sequence name="FaultNotification">
        <reply name="Reply_Fault" faultName="client:fault" variable="generalResponse"
               partnerLink="expireorderprocess_client" portType="client:BasicOrderPort"
               operation="ExpireOrder"/>
        <exit name="Exit"/>
      </sequence>
    </catch>
    <catchAll>
      <scope name="faultNotificationProcessError" exitOnStandardFault="no">
        <bpelx:annotation>
          <bpelx:general>
            <bpelx:property name="userImage">earth.png</bpelx:property>
          </bpelx:general>
        </bpelx:annotation>
        <faultHandlers>
          <catch faultName="ns17:fault" faultVariable="faultVariable"
                 faultElement="ns15:GeneralResponse">
            <sequence name="fault">
              <assign name="AssignGeneralResponse">
                <copy>
                  <from>$uti</from>
                  <to>$generalResponse.fault/uti</to>
                </copy>
                <copy>
                  <from>$faultVariable/status</from>
                  <to>$generalResponse.fault/status</to>
                </copy>
                <copy>
                  <from>$faultVariable/code</from>
                  <to>$generalResponse.fault/code</to>
                </copy>
                <copy>
                  <from>$faultVariable/message</from>
                  <to>$generalResponse.fault/message</to>
                </copy>
              </assign>
              <reply name="ReplyFault" faultName="client:fault"
                     variable="generalResponse"
                     partnerLink="expireorderprocess_client"
                     portType="client:BasicOrderPort" operation="ExpireOrder"/>
              <exit name="Exit1"/>
            </sequence>
          </catch>
        </faultHandlers>
        <sequence name="faultNotificationProcessError">
          <assign name="AssignFaultNotification">
            <copy>
              <from>$uti</from>
              <to>$Invoke_CreateFault_Input.body/uti</to>
            </copy>
            <copy>
              <from><literal>INS_O2_EXPIRE_ORDER_PROCESS</literal></from>
              <to>$Invoke_CreateFault_Input.body/instanceID</to>
            </copy>
            <copy>
              <from><literal>328</literal></from>
              <to>$Invoke_CreateFault_Input.body/errorMappingID</to>
            </copy>
            <copy>
              <from><literal>O2_TEC24</literal></from>
              <to>$Invoke_CreateFault_Input.body/reasonID</to>
            </copy>
            <copy>
              <from>ora:getFaultAsString()</from>
              <to>$Invoke_CreateFault_Input.body/detailError</to>
            </copy>
            <copy>
              <from><literal>3</literal></from>
              <to>$Invoke_CreateFault_Input.body/errorID</to>
            </copy>
            <copy>
              <from><literal>177</literal></from>
              <to>$Invoke_CreateFault_Input.body/serviceID</to>
            </copy>
            <copy>
              <from><literal>O2Service_Bpel</literal></from>
              <to>$Invoke_CreateFault_Input.body/serviceDetailID</to>
            </copy>
            <copy>
              <from><literal>SOA</literal></from>
              <to>$Invoke_CreateFault_Input.body/userName</to>
            </copy>
            <copy>
              <from><literal>faultNotificationProcessError - ExpireOrderProcess</literal></from>
              <to>$Invoke_CreateFault_Input.body/stageName</to>
            </copy>
          </assign>
          <invoke name="InvokeFaultNotification"
                  partnerLink="FaultNotificationService"
                  portType="ns17:FaultNotification_Port" operation="CreateFault"
                  inputVariable="Invoke_CreateFault_Input"
                  outputVariable="Invoke_CreateFault_Output"
                  bpelx:invokeAsDetail="no"/>
        </sequence>
      </scope>
    </catchAll>
  </faultHandlers>
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
     ORCHESTRATION LOGIC                                               
     Set of activities coordinating the flow of messages across the    
     services integrated within this business process                  
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <sequence name="main">
    <!-- Receive input from requestor. (Note: This maps to operation defined in ExpireOrderProcess.wsdl) -->
    <receive name="receiveInput" partnerLink="expireorderprocess_client"
             portType="client:BasicOrderPort" operation="ExpireOrder" variable="inputVariable"
             createInstance="yes"/>
    <!-- Generate reply to synchronous request -->
    <assign name="AssignUti">
      <copy>
        <from>ora:getECID()</from>
        <to>$uti</to>
      </copy>
    </assign>
    <scope name="ConsultarUTI" exitOnStandardFault="no">
      <bpelx:annotation>
        <bpelx:general>
          <bpelx:property name="userDescription">ErrorParametrizado</bpelx:property>
          <bpelx:property name="userImage">database_lg.png</bpelx:property>
        </bpelx:general>
      </bpelx:annotation>
      <faultHandlers>
        <catchAll>
          <scope name="faultNotification" exitOnStandardFault="no">
            <bpelx:annotation>
              <bpelx:general>
                <bpelx:property name="userImage">earth.png</bpelx:property>
              </bpelx:general>
            </bpelx:annotation>
            <faultHandlers>
              <catch faultName="ns17:fault" faultVariable="faultVariable"
                     faultElement="ns15:GeneralResponse">
                <sequence name="fault">
                  <assign name="AssignGeneralResponse">
                    <copy>
                      <from>$uti</from>
                      <to>$generalResponse.fault/uti</to>
                    </copy>
                    <copy>
                      <from>$faultVariable/status</from>
                      <to>$generalResponse.fault/status</to>
                    </copy>
                    <copy>
                      <from>$faultVariable/code</from>
                      <to>$generalResponse.fault/code</to>
                    </copy>
                    <copy>
                      <from>$faultVariable/message</from>
                      <to>$generalResponse.fault/message</to>
                    </copy>
                  </assign>
                  <throw name="FaultNotification" faultName="ns17:fault"
                         faultVariable="globalFaultVar"/>
                </sequence>
              </catch>
            </faultHandlers>
            <sequence name="faultNotification">
              <assign name="AssignFaultNotification">
                <copy>
                  <from>$uti</from>
                  <to>$Invoke_CreateFault_Input.body/uti</to>
                </copy>
                <copy>
                  <from><literal>INS_ORCHESTRATOR_M_SLCT</literal></from>
                  <to>$Invoke_CreateFault_Input.body/instanceID</to>
                </copy>
                <copy>
                  <from><literal>317</literal></from>
                  <to>$Invoke_CreateFault_Input.body/errorMappingID</to>
                </copy>
                <copy>
                  <from><literal>O2_TEC22</literal></from>
                  <to>$Invoke_CreateFault_Input.body/reasonID</to>
                </copy>
                <copy>
                  <from><literal>5</literal></from>
                  <to>$Invoke_CreateFault_Input.body/errorID</to>
                </copy>
                <copy>
                  <from><literal>177</literal></from>
                  <to>$Invoke_CreateFault_Input.body/serviceID</to>
                </copy>
                <copy>
                  <from><literal>O2Service_Bpel</literal></from>
                  <to>$Invoke_CreateFault_Input.body/serviceDetailID</to>
                </copy>
                <copy>
                  <from><literal>SOA</literal></from>
                  <to>$Invoke_CreateFault_Input.body/userName</to>
                </copy>
                <copy>
                  <from><literal>ConsultarUTI - ExpireOrderProcess</literal></from>
                  <to>$Invoke_CreateFault_Input.body/stageName</to>
                </copy>
                <copy>
                  <from>ora:getFaultAsString()</from>
                  <to>$Invoke_CreateFault_Input.body/detailError</to>
                </copy>
              </assign>
              <invoke name="InvokeFaultNotification"
                      partnerLink="FaultNotificationService"
                      portType="ns17:FaultNotification_Port"
                      operation="CreateFault"
                      inputVariable="Invoke_CreateFault_Input"
                      outputVariable="Invoke_CreateFault_Output"
                      bpelx:invokeAsDetail="no"/>
            </sequence>
          </scope>
        </catchAll>
      </faultHandlers>
      <sequence name="ConsultarUTI">
        <assign name="Assign_LogMSelectReq">
          <copy>
            <from>$inputVariable.body/ns4:basicRequest/utiReference</from>
            <to>$Invoke_LogMSelect_Input.LOG_ORCHESTRATOR_M_ADAPTERSelect_inputParameters/ns2:uti</to>
          </copy>
        </assign>
        <invoke name="Invoke_LogMSelect" partnerLink="LOG_ORCHESTRATOR_M_ADAPTER"
                portType="ns1:LOG_ORCHESTRATOR_M_ADAPTER_ptt"
                operation="LOG_ORCHESTRATOR_M_ADAPTERSelect" inputVariable="Invoke_LogMSelect_Input"
                outputVariable="Invoke_LogMSelect_Output" bpelx:invokeAsDetail="no"/>
      </sequence>
    </scope>
    <if name="UTI">
      <documentation>Exist</documentation>
      <condition>ora:countNodes('Invoke_LogMSelect_Output','LogOrchestratorMCollection','ns2:LogOrchestratorM') &gt; 0</condition>
      <sequence name="ConsultarProductos">
        <scope name="ConsultarProductos" exitOnStandardFault="no">
          <bpelx:annotation>
            <bpelx:general>
              <bpelx:property name="userDescription">ErrorParametrizado</bpelx:property>
              <bpelx:property name="userImage">database_lg.png</bpelx:property>
            </bpelx:general>
          </bpelx:annotation>
          <faultHandlers>
            <catchAll>
              <scope name="FaultNotification" exitOnStandardFault="no">
                <bpelx:annotation>
                  <bpelx:general>
                    <bpelx:property name="userImage">earth.png</bpelx:property>
                  </bpelx:general>
                </bpelx:annotation>
                <faultHandlers>
                  <catch faultName="ns17:fault" faultVariable="faultVariable"
                         faultElement="ns15:GeneralResponse">
                    <sequence name="fault">
                      <assign name="AssignGeneralResponse">
                        <copy>
                          <from>$uti</from>
                          <to>$generalResponse.fault/uti</to>
                        </copy>
                        <copy>
                          <from>$faultVariable/status</from>
                          <to>$generalResponse.fault/status</to>
                        </copy>
                        <copy>
                          <from>$faultVariable/code</from>
                          <to>$generalResponse.fault/code</to>
                        </copy>
                        <copy>
                          <from>$faultVariable/message</from>
                          <to>$generalResponse.fault/message</to>
                        </copy>
                      </assign>
                      <throw name="FaultNotification" faultName="ns17:fault"
                             faultVariable="globalFaultVar"/>
                    </sequence>
                  </catch>
                </faultHandlers>
                <sequence name="FaultNotification">
                  <assign name="AssignFaultNotification">
                    <copy>
                      <from>$uti</from>
                      <to>$Invoke_CreateFault_Input.body/uti</to>
                    </copy>
                    <copy>
                      <from><literal>5</literal></from>
                      <to>$Invoke_CreateFault_Input.body/errorID</to>
                    </copy>
                    <copy>
                      <from><literal>177</literal></from>
                      <to>$Invoke_CreateFault_Input.body/serviceID</to>
                    </copy>
                    <copy>
                      <from><literal>O2Service_Bpel</literal></from>
                      <to>$Invoke_CreateFault_Input.body/serviceDetailID</to>
                    </copy>
                    <copy>
                      <from><literal>SOA</literal></from>
                      <to>$Invoke_CreateFault_Input.body/userName</to>
                    </copy>
                    <copy>
                      <from><literal>ConsultarProductos - ExpireOrderProcess</literal></from>
                      <to>$Invoke_CreateFault_Input.body/stageName</to>
                    </copy>
                    <copy>
                      <from>ora:getFaultAsString()</from>
                      <to>$Invoke_CreateFault_Input.body/detailError</to>
                    </copy>
                    <copy>
                      <from><literal>INS_ORCHESTRATOR_PRODUCT_SLCT</literal></from>
                      <to>$Invoke_CreateFault_Input.body/instanceID</to>
                    </copy>
                    <copy>
                      <from><literal>O2_TEC23</literal></from>
                      <to>$Invoke_CreateFault_Input.body/reasonID</to>
                    </copy>
                    <copy>
                      <from><literal>318</literal></from>
                      <to>$Invoke_CreateFault_Input.body/errorMappingID</to>
                    </copy>
                  </assign>
                  <invoke name="InvokeFaultNotification"
                          partnerLink="FaultNotificationService"
                          portType="ns17:FaultNotification_Port"
                          operation="CreateFault"
                          inputVariable="Invoke_CreateFault_Input"
                          outputVariable="Invoke_CreateFault_Output"
                          bpelx:invokeAsDetail="no"/>
                </sequence>
              </scope>
            </catchAll>
          </faultHandlers>
          <sequence name="ConsultarProductos">
            <assign name="Assign_LogProductReq">
              <copy>
                <from>$inputVariable.body/ns4:basicRequest/utiReference</from>
                <to>$Invoke_LOG_ORCHESTRATOR_PRODUCT_SELECT_Input.LOG_ORCHESTRATOR_PRODUCT_SELECTInput_msg/ns19:correlationId</to>
              </copy>
            </assign>
            <invoke name="Invoke_LogProductSlect"
                    partnerLink="LOG_ORCHESTRATOR_PRODUCT_SELECT"
                    portType="ns18:LOG_ORCHESTRATOR_PRODUCT_SELECT_ptt"
                    operation="LOG_ORCHESTRATOR_PRODUCT_SELECT"
                    inputVariable="Invoke_LOG_ORCHESTRATOR_PRODUCT_SELECT_Input"
                    outputVariable="Invoke_LOG_ORCHESTRATOR_PRODUCT_SELECT_Output"
                    bpelx:invokeAsDetail="no"/>
          </sequence>
        </scope>
        <forEach parallel="no" counterName="productCounter" name="Productos">
          <startCounterValue>1</startCounterValue>
          <finalCounterValue>ora:countNodes('Invoke_LOG_ORCHESTRATOR_PRODUCT_SELECT_Output','LOG_ORCHESTRATOR_PRODUCT_SELECTOutputCollection','ns19:LOG_ORCHESTRATOR_PRODUCT_SELECTOutput')</finalCounterValue>
          <scope name="Productos" exitOnStandardFault="no">
            <sequence name="Productos">
              <scope name="ProvisioningService" exitOnStandardFault="no">
                <bpelx:annotation>
                  <bpelx:general>
                    <bpelx:property name="userDescription">ErrorParametrizado</bpelx:property>
                    <bpelx:property name="userImage">earth.png</bpelx:property>
                  </bpelx:general>
                </bpelx:annotation>
                <faultHandlers>
                  <catch faultName="ns16:fault" faultVariable="faulVariable"
                         faultElement="ns15:GeneralResponse">
                    <sequence name="ReplyFault">
                      <assign name="Assign_Fault">
                        <copy>
                          <from>$faulVariable/status</from>
                          <to>$generalResponse.fault/status</to>
                        </copy>
                        <copy>
                          <from>$faulVariable/code</from>
                          <to>$generalResponse.fault/code</to>
                        </copy>
                        <copy>
                          <from>$faulVariable/message</from>
                          <to>$generalResponse.fault/message</to>
                        </copy>
                        <copy>
                          <from>ora:getConversationId()</from>
                          <to>$generalResponse.fault/uti</to>
                        </copy>
                      </assign>
                      <throw name="FaultNotification" faultName="ns17:fault"
                             faultVariable="globalFaultVar"/>
                    </sequence>
                  </catch>
                  <catchAll>
                    <scope name="FaultNotification" exitOnStandardFault="no">
                      <bpelx:annotation>
                        <bpelx:general>
                          <bpelx:property name="userImage">earth.png</bpelx:property>
                        </bpelx:general>
                      </bpelx:annotation>
                      <faultHandlers>
                        <catch faultName="ns17:fault"
                               faultVariable="faultVariable"
                               faultElement="ns15:GeneralResponse">
                          <sequence name="fault">
                            <assign name="AssignGeneralResponse">
                              <copy>
                                <from>$uti</from>
                                <to>$generalResponse.fault/uti</to>
                              </copy>
                              <copy>
                                <from>$faultVariable/status</from>
                                <to>$generalResponse.fault/status</to>
                              </copy>
                              <copy>
                                <from>$faultVariable/code</from>
                                <to>$generalResponse.fault/code</to>
                              </copy>
                              <copy>
                                <from>$faultVariable/message</from>
                                <to>$generalResponse.fault/message</to>
                              </copy>
                            </assign>
                            <throw name="FaultNotification"
                                   faultName="ns17:fault"
                                   faultVariable="globalFaultVar"/>
                          </sequence>
                        </catch>
                      </faultHandlers>
                      <sequence name="FaultNotification">
                        <assign name="AssignFaultNotification">
                          <copy>
                            <from>$uti</from>
                            <to>$Invoke_CreateFault_Input.body/uti</to>
                          </copy>
                          <copy>
                            <from><literal>ProvisioningService - ExpireOrderProcess</literal></from>
                            <to>$Invoke_CreateFault_Input.body/stageName</to>
                          </copy>
                          <copy>
                            <from>ora:getFaultAsString()</from>
                            <to>$Invoke_CreateFault_Input.body/detailError</to>
                          </copy>
                          <copy>
                            <from><literal>INS_O2_PROVISIONING</literal></from>
                            <to>$Invoke_CreateFault_Input.body/instanceID</to>
                          </copy>
                          <copy>
                            <from><literal>265</literal></from>
                            <to>$Invoke_CreateFault_Input.body/errorMappingID</to>
                          </copy>
                          <copy>
                            <from><literal>O2_TEC15</literal></from>
                            <to>$Invoke_CreateFault_Input.body/reasonID</to>
                          </copy>
                          <copy>
                            <from><literal>6</literal></from>
                            <to>$Invoke_CreateFault_Input.body/errorID</to>
                          </copy>
                          <copy>
                            <from><literal>177</literal></from>
                            <to>$Invoke_CreateFault_Input.body/serviceID</to>
                          </copy>
                          <copy>
                            <from><literal>O2Service_Bpel</literal></from>
                            <to>$Invoke_CreateFault_Input.body/serviceDetailID</to>
                          </copy>
                          <copy>
                            <from><literal>SOA</literal></from>
                            <to>$Invoke_CreateFault_Input.body/userName</to>
                          </copy>
                        </assign>
                        <invoke name="InvokeFaultNotification"
                                partnerLink="FaultNotificationService"
                                portType="ns17:FaultNotification_Port"
                                operation="CreateFault"
                                inputVariable="Invoke_CreateFault_Input"
                                outputVariable="Invoke_CreateFault_Output"
                                bpelx:invokeAsDetail="no"/>
                      </sequence>
                    </scope>
                  </catchAll>
                </faultHandlers>
                <sequence name="ProvisioningService">
                  <assign name="AssignProvisioningReq">
                    <copy>
