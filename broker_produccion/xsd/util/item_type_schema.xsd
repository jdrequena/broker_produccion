<?xml version='1.0' encoding='UTF-8'?>
<xsd:schema version="1.0" targetNamespace="http://tigo.com/enterprise/business/entities/order/v1/schema" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:cli="http://tigo.com/enterprise/business/entities/client/basic/v1/schema" xmlns:ord="http://tigo.com/enterprise/business/entities/order/v1/schema" xmlns:prm="http://tigo.com/enterprise/resources/parameters/simple/v1/schema" xmlns:not="http://tigo.com/enterprise/business/entities/notificationBasic/v1/schema">
   <xsd:import namespace="http://tigo.com/enterprise/resources/parameters/simple/v1/schema" schemaLocation="../partner/resources/Parameters.xsd"/>
   <xsd:import namespace="http://tigo.com/enterprise/business/entities/client/basic/v1/schema" schemaLocation="../partner/client/BasicClient_schema.xsd"/>
   <xsd:import namespace="http://tigo.com/enterprise/business/entities/notificationBasic/v1/schema" schemaLocation="../partner/NotificationBasic_schema.xsd"/>
   <xsd:element name="item" type="ord:orderItem_type"/>
   <xsd:complexType name="orderItem_type">
      <xsd:sequence>
         <xsd:element name="itemID" type="xsd:string" minOccurs="0" maxOccurs="1"/>
         <xsd:element name="itemStatus" type="ord:orderStatus_type" minOccurs="0" maxOccurs="1"/>
